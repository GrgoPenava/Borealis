@model IEnumerable<Users>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Users</h1>

<table class="table table-boardered table-stripped" style="width:100%">
    <thead>
        <tr>
            <th>
                Username
            </th>
            <th>
                Lozinka
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            
            <tr>
                <td width="50%">
                    @item.Username
                </td>
                <td width="50%">
                    @item.lozinka
                </td>
            </tr>
        }
    </tbody>
</table>

